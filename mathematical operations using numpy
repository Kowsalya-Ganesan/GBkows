import numpy as np
x=np.array([[1,2,3,4],[5,6,7,8]])
y=np.array([[9,10,11,12],[13,14,15,16]])
print(np.add(x,y))
'''[[10 12 14 16]
   [18 20 22 24]]'''
print(np.reaminder(y,x))
'''[[0 0 2 0]
   [3 2 1 0]]'''
print(x**2)
'''[[1 4 9 16]
   [25 36 49 64]]'''
print(y-x)
print(x<5)
'''[[True True True True]
   [False False False False]]'''
